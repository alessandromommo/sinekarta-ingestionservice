<jxb:bindings 
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
    jxb:extensionBindingPrefixes="xjc"
    xmlns:xml="http://www.w3.org/XML/1998/namespace"
    version="2.1">
    
    <jxb:bindings schemaLocation="mets.xsd">
    	<jxb:bindings node="//xsd:complexType[@name='metsType']">
			<jxb:bindings node=".//xsd:attribute[@name='LABEL']">
				<jxb:property name="LABEL1"/>
			</jxb:bindings>
		</jxb:bindings>
		
		<jxb:bindings node="//xsd:complexType[@name='structMapType']">
			<jxb:bindings node=".//xsd:attribute[@name='LABEL']">
				<jxb:property name="LABEL2"/>
			</jxb:bindings>
		</jxb:bindings>
		
		<jxb:bindings node="//xsd:complexType[@name='divType']">
			<jxb:bindings node=".//xsd:attribute[@name='LABEL']">
				<jxb:property name="LABEL3"/>
			</jxb:bindings>
		</jxb:bindings>
		
		<jxb:bindings node="//xsd:complexType[@name='behaviorSecType']">
			<jxb:bindings node=".//xsd:attribute[@name='LABEL']">
				<jxb:property name="LABEL4"/>
			</jxb:bindings>
		</jxb:bindings>
		
		<jxb:bindings node="//xsd:complexType[@name='behaviorType']">
			<jxb:bindings node=".//xsd:attribute[@name='LABEL']">
				<jxb:property name="LABEL5"/>
			</jxb:bindings>
		</jxb:bindings>
		
		<jxb:bindings node="//xsd:complexType[@name='objectType']">
			<jxb:bindings node=".//xsd:attribute[@name='LABEL']">
				<jxb:property name="LABEL6"/>
			</jxb:bindings>
		</jxb:bindings>
		
		<jxb:bindings node="//xsd:element[@name='mdRef']">
			<jxb:bindings node=".//xsd:attribute[@name='LABEL']">
				<jxb:property name="LABEL7"/>
			</jxb:bindings>
		</jxb:bindings>
		
		<jxb:bindings node="//xsd:element[@name='mdWrap']">
			<jxb:bindings node=".//xsd:attribute[@name='LABEL']">
				<jxb:property name="LABEL8"/>
			</jxb:bindings>
		</jxb:bindings>
		
		
	</jxb:bindings>
</jxb:bindings>